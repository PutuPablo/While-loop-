// Break
for (int i = 0; i < 10; i++)
{
    Console.WriteLine(i);
    if (i == 3)
    {
        Console.WriteLine("I've had enough!");
        break;
    }
    
}
Console.ReadKey();



////////////////////////////////////////////////////////////////////



// Continue
for (int i = 0; i < 4; i++)
{
    Console.WriteLine(i);
    if (i == 2)
    {
        continue;
    }
    Console.WriteLine(i);
}
Console.ReadKey();

